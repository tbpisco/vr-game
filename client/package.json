    "include": [
      "./src/**/*.js"
    ],
    "exclude": [
      "./src/**/*.spec.js"
    ],
    "instrument": false,
    "sourceMap": false,
    "all": true
  },
  "scripts": {
    "test": "mocha-webpack --webpack-config webpack.config.test.js \"src/**/*.spec.js\"",
    "watch": "mocha-webpack --webpack-config webpack.config.test.js --watch \"src/**/*.spec.js\" || true",
    "build-dev": "webpack --config webpack.config.js",
    "build-prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "start": "webpack-dev-server",
    "precommit": "npm run test",
    "cover": "cross-env NODE_ENV=coverage nyc --reporter=lcov --reporter=text npm run test"
